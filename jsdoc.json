{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["./src"],
      "includePattern": "\\.(jsx|js|ts|tsx)$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "better-docs/category", "better-docs/typescript"],
    "templates": {
      "default": {
        "staticFiles": {
          "include": [
            "./doc_files/wap.png"
          ]
        }
      },
      "better-docs": {
        "name": "WAP - 2",
        "navLinks": [
          {
              "label": "Github",
              "href": "https://github.com/dejvones/wap-2"
          },
          {
              "label": "Online aplikace",
              "href": "https://bespoke-conkies-73ae11.netlify.app/"
          }
      ]
      }
    },
    "opts": {
      "destination": "doc",
      "recurse": true,
      "readme": "./doc_files/README.md",
      "template": "./node_modules/better-docs"
    }
  }